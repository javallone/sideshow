<% if unassociated.length > 0 %>
    <div data-role="collapsible" data-collapsed="true" data-theme="b" data-content-theme="d">
        <h3><%= unassociated.length %> Unassociated Files</h3>

        <ul>
            <% unassociated.each do |movie| %>
                <li><%= movie.file %></li>
            <% end %>
        </ul>
    </div>
<% end %>

<%= erb :program_list, :layout => false, :locals => { :programs => programs } %>
